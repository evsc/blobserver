<Blobserver>
    <Flow>
        <Detector>
            <Type>Detector_Hog</Type>
            <Param>
                <Name>verbose</Name>
                <Value>0</Value>
            </Param>

            <Param>
                <Name>modelFilename</Name>
                <Value>model.xml</Value>
            </Param>
            <Param>
                <Name>maxTimePerFrame</Name>
                <Value>1e5</Value>
            </Param>
            <Param>
                <Name>maxThreads</Name>
                <Value>8</Value>
            </Param>
            <Param>
                <Name>mergeDistance</Name>
                <Value>64</Value>
            </Param>
        </Detector>
        <Source>
            <Type>Source_OpenCV</Type>
            <Subsource>300</Subsource>
            <Param>
                <Name>framerate</Name>
                <Value>30</Value>
            </Param>
            <Param>
                <Name>iso</Name>
                <Value>800</Value>
            </Param>
            <Param>
                <Name>width</Name>
                <Value>640</Value>
            </Param>
            <Param>
                <Name>height</Name>
                <Value>480</Value>
            </Param>
            <Param>
                <Name>exposureTime</Name>
                <Value>500</Value>
            </Param>
            <Param>
                <Name>gain</Name>
                <Value>512</Value>
            </Param>
            <Param>
                <Name>whiteBalanceRed</Name>
                <Value>800</Value>
            </Param>
            <Param>
                <Name>whiteBalanceBlue</Name>
                <Value>512</Value>
            </Param>
        </Source>
        <Client>
            <Address>localhost</Address>
            <Port>9000</Port>
        </Client>
        <Server>
            <Address>localhost</Address>
            <Port>9002</Port>
        </Server>
    </Flow>
</Blobserver>
