            </Param>
            <Param>
                <Name>gamma</Name>
                <Value>1.0</Value>
            </Param>
            <Param>
                <Name>whiteBalanceRed</Name>
                <Value>512</Value>
            </Param>
            <Param>
                <Name>whiteBalanceBlue</Name>
                <Value>512</Value>
            </Param>
