            </Param>
            <Param>
                <Name>iso</Name>
                <Value>400</Value>
            </Param>
            <Param>
                <Name>width</Name>
                <Value>640</Value>
            </Param>
            <Param>
                <Name>height</Name>
                <Value>480</Value>
            </Param>
            <Param>
                <Name>exposureTime</Name>
                <Value>512</Value>
            </Param>
